{
  "author": "reyes",
  "dependencies": {
    "@nestjs/typeorm": "10.0.2",
    "@aws-sdk/client-s3": "^3.606.0",
    "@aws-sdk/s3-request-presigner": "^3.606.0",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/common": "^10.3.9",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.9",
    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.9",
    "@nestjs/platform-socket.io": "^10.3.9",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/typeorm": "^10.0.2",
    "@nestjs/websockets": "^10.3.9",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.6",
    "dd-trace": "^5.17.0",
    "dotenv": "^16.4.5",
    "google-auth-library": "^9.11.0",
    "jsonwebtoken": "^9.0.2",
    "node-mailjet": "^6.0.5",
    "nodemailer": "^6.9.14",
    "openai": "^4.52.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.5",
    "stripe": "^16.1.0",
    "typeorm": "^0.3.20",
    "uuid": "^10.0.0",
    "winston": "^3.13.0"
  },
  "description": "Marblism server",
  "devDependencies": {
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.9",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.14.9",
    "@types/nodemailer": "^6.4.15",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.2",
    "webpack": "^5.92.1"
  },
  "engines": {
    "node": "20.11.0",
    "npm": "10.2.4"
  },
  "files": [
    "dist"
  ],
  "name": "server",
  "private": true,
  "scripts": {
    "build": "nest build",
    "check": "tsc",
    "check:dependencies": "ts-node -r tsconfig-paths/register ./scripts/check-dependencies.ts",
    "database:init": "npm run database:schema:destroy && npm run database:schema:create",
    "database:migration:build": "sh ./scripts/database.migration.sh && ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate ./src/core/database/migrations/scripts/script -d ./src/migration.ts",
    "database:migration:revert": "sh ./scripts/database.migration.sh && ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert -d ./src/migration.ts",
    "database:migration:run": "sh ./scripts/database.migration.sh && ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d ./src/migration.ts",
    "database:schema:create": "sh ./scripts/database.migration.sh && ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js schema:sync -d ./src/migration.ts",
    "database:schema:destroy": "sh ./scripts/database.migration.sh && ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js schema:drop -d ./src/migration.ts",
    "dev": "nest start --debug --watch",
    "docker:init": "docker compose -p marblism-app up -d",
    "ensure": "pnpm -v || npm install -g pnpm && pnpm install",
    "env:init": "sh ./scripts/env.sh",
    "format": "prettier --write \"src/**/*.ts\"",
    "init": "pnpm run docker:init ; pnpm env:init ; pnpm run database:migration:run",
    "install": "npm i",
    "start": "node dist/main",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0"
  },
  "version": "0.0.1"
}